{
  "name": "protractor-es6modules-typescript-tsnode-jasmine",
  "version": "1.0.0",
  "description": "Protractor end-to-end test automation boilerplate project. Using: ES6 modules, TypeScript, ts-node & jasmine",
  "scripts": {
    "upadte-webderiver": "npx webdriver-manager update",
    "test": "npx protractor ./protractor.conf.js",
    "lint": "npx tslint -c tslint.json './spec/*.ts'",
    "pretty": "npx prettier --no-config './spec/*.ts' --write"
  },
  "keywords": [
    "protractor",
    "es6modules",
    "typescript",
    "ts-node",
    "jasmine",
    "boilerplate",
    "example"
  ],
  "author": "Girma Nigusse <xgirma@gmail.com> (https://github.com/xgirma)",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "3.5.0",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.22",
    "prettier": "1.19.1",
    "protractor": "5.4.2",
    "rimraf": "3.0.0",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.4"
  }
}
