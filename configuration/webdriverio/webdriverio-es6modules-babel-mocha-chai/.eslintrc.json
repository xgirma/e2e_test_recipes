{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended",
    "plugin:wdio/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "$": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": [
    "wdio"
  ]
}
