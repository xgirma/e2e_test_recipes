    "cypress": "3.8.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-cypress": "2.8.1",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "prettier": "1.19.1",
    "rimraf": "3.0.1"
  }
