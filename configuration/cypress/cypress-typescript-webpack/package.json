{
  "name": "cypress-typescript-webpack",
  "version": "1.0.0",
  "description": "cypress end-to-end test automation boilerplate project. Using TypeScript and webpack",
  "main": "index.js",
  "scripts": {
    "clean:video": "npx rimraf cypress/videos/*",
    "clean:screenshots": "npx cypress/screenshots/*",
    "open": "npx cypress open",
    "run": "npx cypress run",
    "lint": "tslint -c tslint.json './cypress/integration/*.ts'",
    "pretty": "prettier --no-config './cypress/**/*.ts' --write"
  },
  "keywords": [
    "cypress",
    "typescript",
    "webpack",
    "boilerplate",
    "example"
  ],
  "author": "Girma Nigusse <xgirma@gmail.com> (https://github.com/xgirma)",
  "license": "MIT",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "4.1.0",
    "cypress": "3.6.1",
    "prettier": "1.18.2",
    "ts-loader": "6.2.1",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.2",
    "webpack": "4.41.2"
  }
}
